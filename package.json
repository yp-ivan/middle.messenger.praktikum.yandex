{
  "name": "messenger",
  "version": "1.0.0",
  "description": "Проект Messenger (практика Yandex.Praktikum).",
  "scripts": {
    "serve": "node ./server.js",
    "dev": "webpack --mode=development",
    "build": "webpack --mode=production --node-env=production",
    "start": "webpack serve",
    "check:eslint": "npx eslint src",
    "check:stylelint": "npx stylelint src/**/*.scss",
    "check:test": "echo \"Error: no test specified\" && exit 1",
    "check": "npm-run-all --parallel check:*",
    "prepare": "husky install",
    "build:dev": "npm run dev",
    "build:prod": "npm run build",
    "watch": "webpack --watch"
  },
  "alias": {
    "assets": "./src/assets",
    "api": "./src/api",
    "components": "./src/components",
    "core": "./src/core",
    "data": "./src/data",
    "helpers": "./src/helpers",
    "pages": "./src/pages",
    "router": "./src/router",
    "services": "./src/services",
    "store": "./src/store"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yp-ivan/middle.messenger.praktikum.yandex.git"
  },
  "keywords": [],
  "author": "Ivan Gavrilov",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/yp-ivan/middle.messenger.praktikum.yandex/issues"
  },
  "homepage": "https://github.com/yp-ivan/middle.messenger.praktikum.yandex#readme",
  "devDependencies": {
    "@types/node": "18.11.18",
    "@types/uuid": "9.0.1",
    "@typescript-eslint/eslint-plugin": "5.50.0",
    "@typescript-eslint/parser": "5.50.0",
    "css-loader": "6.7.1",
    "eslint": "8.33.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "17.0.0",
    "html-webpack-plugin": "5.5.0",
    "husky": "8.0.3",
    "mini-css-extract-plugin": "2.6.1",
    "npm-run-all": "4.1.5",
    "sass": "1.59.2",
    "sass-loader": "13.0.2",
    "style-loader": "3.3.1",
    "stylelint": "14.16.1",
    "stylelint-config-standard": "29.0.0",
    "stylelint-config-standard-scss": "6.1.0",
    "stylelint-scss": "4.3.0",
    "ts-loader": "9.4.1",
    "typescript": "4.9.5",
    "webpack": "5.76.1",
    "webpack-cli": "5.0.1",
    "webpack-dev-server": "4.10.0"
  },
  "dependencies": {
    "express": "4.18.2",
    "handlebars": "4.7.7",
    "uuid": "9.0.0"
  }
