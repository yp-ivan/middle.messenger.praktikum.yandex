{
  "name": "messenger",
  "version": "1.0.0",
  "description": "Проект Messenger (практика Yandex.Praktikum).",
  "scripts": {
    "serve": "node ./server.js",
    "dev": "parcel src/index.html --no-cache",
    "build": "parcel build src/index.html --no-source-maps --public-url ./",
    "start": "npm run build && npm run serve",
    "eslint": "npx eslint src",
    "stylelint": "npx stylelint src/**/*.scss"
  },
  "alias": {
    "core": "./src/core",
    "components": "./src/components",
    "helpers": "./src/helpers",
    "pages": "./src/pages",
    "data": "./src/data",
    "assets": "./src/assets"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yp-ivan/middle.messenger.praktikum.yandex.git"
  },
  "keywords": [],
  "author": "Ivan Gavrilov",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/yp-ivan/middle.messenger.praktikum.yandex/issues"
  },
  "homepage": "https://github.com/yp-ivan/middle.messenger.praktikum.yandex#readme",
  "devDependencies": {
    "@anikin/parcel-transformer-handlebars": "1.0.9",
    "@babel/core": "7.20.12",
    "@parcel/transformer-inline-string": "2.7.0",
    "@parcel/transformer-sass": "2.7.0",
    "@types/node": "18.11.18",
    "@typescript-eslint/eslint-plugin": "5.50.0",
    "@typescript-eslint/parser": "5.50.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "eslint": "8.33.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "17.0.0",
    "parcel": "2.7.0",
    "parcel-plugin-handlebars-precompile": "1.0.2",
    "stylelint": "14.16.1",
    "stylelint-config-standard": "29.0.0",
    "stylelint-config-standard-scss": "6.1.0",
    "stylelint-scss": "4.3.0",
    "typescript": "4.9.5"
  },
  "dependencies": {
    "babel-core": "6.26.3",
    "express": "4.18.2",
    "handlebars": "4.7.7",
    "nanoid": "4.0.0"
  }
